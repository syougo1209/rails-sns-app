<h1>chatroom</h1>
<form class="form-inline">
    <div class="form-group">
      <input type="text" 
      class="form-control"
      id="chat-input"
      data_user="<%= current_user.id %>"
      data_room="<%= params[:id] %>"/>
    </div>
    <button id="button" class="btn btn-primary" >送信</button>
  </form>
  
<div id="messages">
    <ul class="list-group">
    <%= render @messages %>
    </ul>
</div>
