<h1>ユーザー検索</h1>
<section id="posts-search">
    <div class="row">
<%= form_tag(users_path, :method => "get") do %>
 <div class="form-group ranking-form">
<label for="search-post-form">キーワード</label>
 <%= text_field_tag :search,"" ,class: "form-control",id: "search-post-form" %> 
 </div>
 <div class="form-group ranking-form">
 <label for="place-post-form">県名</label>
 <%= render "shared/prefecture_form" %>
 </div>
 <div>
<%= submit_tag "検索" ,class: "post-search-buttton button btn-primary col-xs-offset-3 col-xs-6"%> 
</div>
<% end %> 
</div>
</section>



<section class="users">
    <% if @users %>
    <%= render @users %>
    <%= will_paginate %>
    <% end %>
</section>
</div>